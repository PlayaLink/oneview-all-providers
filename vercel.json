{
  "framework": "vite",
  "buildCommand": "bash scripts/build-with-git-branch.sh npm run build",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_GIT_BRANCH": "$(git branch --show-current)",
    "VITE_VERCEL_GIT_COMMIT_REF": "$VERCEL_GIT_COMMIT_REF"
  }
} 